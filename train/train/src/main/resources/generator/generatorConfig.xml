<!--        <table tableName="orders"></table>-->
<!--        <table tableName="orderinfo"></table>-->
<!--        <table tableName="train"></table>-->
<!--        <table tableName="traininfo"></table>-->
<!--        <table tableName="trainticket"></table>-->
<!--        <table tableName="trainstate"></table>-->
<!--        <table tableName="trainseat"></table>-->
<!--        <table tableName="orderseat"></table>-->
<!--        <table tableName="seatinfo"></table>-->
<!--        <table tableName="holiday"></table>-->
        <table tableName="pricemanage"></table>

    </context>

</generatorConfiguration>
